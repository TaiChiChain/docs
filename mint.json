{
  "$schema": "https://mintlify.com/schema.json",
  "name": "OpenHub",
  "logo": {
    "dark": "/logo/dark.svg",
    "light": "/logo/light.svg"
  },
  "favicon": "/favicon.png",
  "colors": {
    "primary": "#12A7D3",
    "light": "#81D7F7",
    "dark": "#004E7A"
  },

  "primaryTab": {
    "name": "首页"
  },
  "versions": ["v2", "v1"],
  "anchors": [
    {
      "name": "基础主题",
      "icon": "shield-halved",
      "url": "essentials"
    },
    {
      "name": "堆栈",
      "icon": "code",
      "url": "base"
    },
    {
      "name": "文档站",
      "icon": "puzzle-piece",
      "url": "documents"
    },
    {
      "name": "api",
      "icon": "newspaper",
      "url": "api-reference"
    }
  ],
  "navigation": [
    {
      "version": "v2",
      "group": "基础主题",
      "pages": ["essentials/markdown", "essentials/code", "essentials/images", "essentials/settings", "essentials/navigation"]
    },
    {
      "version": "v1",
      "group": "v1主题",
      "pages": ["essentials/markdown", "essentials/code"]
    },
    {
      "group": "堆栈",
      "pages": ["base/markdown",{
        "group": "智能合约",
        "pages": ["base/code", {
          "group": "合约库",
          "pages": ["base/images", "base/settings", "base/navigation"]
        }]
      }]
    },
    {
      "group": "文档站",
      "pages": ["documents/authentication"]
    },
    {
      "group": "API Documentation",
      "pages": ["api-reference/authentication"]
    },
    {
      "group": "Endpoint Examples",
      "pages": [
        "api-reference/endpoint/get",
        "api-reference/endpoint/create",
        "api-reference/endpoint/update",
        "api-reference/endpoint/delete"
      ]
    }
  ],
  "footerSocials": {
    "github": "https://github.com/TaiChiChain"
  },
  "backgroundImage": "/background.png"
}